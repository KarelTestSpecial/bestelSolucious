{
  "manifest_version": 3,
  "name": "Bestel Tracker",
  "version": "1.0",
  "description": "Een extensie om bestellingen en voorraad te beheren.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "host_permissions": [
    "http://localhost:3000/"
  ]
}
